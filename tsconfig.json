{
  "compilerOptions": {
    "target": "ES2020",
    "outDir": "tsc_build",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "rootDir": ".",
    "allowJs": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "noEmit": true,
    "typeRoots": [
      "./node_modules/@types"
    ]
  },
  "types": [
    "webpack-env"
  ],
  "include": [
    "src",
    "electron"
  ],
  "exclude": [
    "node_modules",
    "build"
  ],
  "extends": "./node_modules/electron-webpack/tsconfig-base.json"
}
